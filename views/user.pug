extends layout

block layout-content
  .view.user-view
    h1.banner Howdy, #{userProfile.nickname}!
    div.message
      div.title
        h3 Making Us the Best
        h1 Teammate Profile
      pre.details=JSON.stringify(userProfile, null, 2)
    div.nav-buttons
      a(href="/logout")
        div.nav-button Log out
